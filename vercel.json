{ 
  "$schema": "https://openapi.vercel.sh/vercel.json", 
  "version": 2,
  "rewrites": [{ "source": "/(.*)", "destination": "/api" }],
  "buildCommand": "yum install -y traceroute",
  "functions": {
    "api": {
      "maxDuration": 10,
      "memory": 1024,
      "runtime": "nodejs18.x"
    }
  },
  "outputDirectory": "api"
} 